{   "name": "contudo-desktop",   "version": "1.0.0",   "description": "Contudo: Aplicativo desktop para gerenciar seu dinheiro de forma simples e rápida",   "main": "main.js",   "scripts": {     "start": "electron .",     "dev": "concurrently \"nodemon main.js\" \"wait-on http://localhost:3001 && electron .\"",     "build": "electron-builder",     "build:win": "electron-builder --win",     "build:mac": "electron-builder --mac",     "build:linux": "electron-builder --linux",     "dist": "npm run build",     "pack": "electron-builder --dir",     "postinstall": "electron-builder install-app-deps"   },   "build": {     "appId": "com.luizpaulo.contudo",     "productName": "Contudo",     "directories": {       "output": "dist"     },     "files": [       "main.js",       "preload.js",       "frontend/**/*",       "assets/**/*",       "node_modules/**/*"     ],     "win": {       "icon": "assets/icon.ico",       "target": [         {           "target": "nsis",           "arch": [             "x64",             "ia32"           ]         },         {           "target": "portable",           "arch": [             "x64"           ]         }       ],       "publish": null     },     "nsis": {       "oneClick": false,       "perMachine": false,       "allowElevation": true,       "allowToChangeInstallationDirectory": true,       "installerIcon": "assets/icon.ico",       "uninstallerIcon": "assets/icon.ico",       "installerHeaderIcon": "assets/icon.ico",       "createDesktopShortcut": true,       "createStartMenuShortcut": true,       "shortcutName": "Contudo"     },     "mac": {       "icon": "assets/icon.icns",       "category": "public.app-category.finance",       "target": [         {           "target": "dmg",           "arch": [             "x64",             "arm64"           ]         }       ]     },     "linux": {       "icon": "assets/icon.png",       "target": [         {           "target": "AppImage",           "arch": [             "x64"           ]         },